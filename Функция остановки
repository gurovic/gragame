def go(x1, y1, x2, y2):
    if color(x1, y1 - 5) == black and color(x2, y2 + 5) == black and color(x2 + 5, y2) == black:
        pass
    if color(x1, y1 - 5) == black and color(x2, y2 + 5) == black and color(x1 - 5, y1) == black:
        pass
    if color(x1 - 5, y1) == black and color(x2 + 5, y2) == black and color(x1, y1 - 5) == black:
        pass
    if color(x1 - 5, y1) == black and color(x2 + 5, y2) == black and color(x2, y2 + 5) == black:
        pass
